generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users{
  id         Int      @id @default(autoincrement())
  name       String
  username   String   
  email      String   
  password   String
  avatar     String?  // "?" significa opcional
  background String?  // opcional
  createdAt  DateTime @default(now())
}